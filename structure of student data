#include <stdio.h>

struct StudentData {
    char *student_name;
    int student_id;
    int student_age;
};
int main()
{
     
     struct StudentData student;

     
     student.student_name = "Virat";
     student.student_id = 1243;
     student.student_age = 27;

     
     printf("Student Name is: %s", student.student_name);
     printf("\nStudent Id is: %d", student.student_id);
     printf("\nStudent Age is: %d", student.student_age);
     return 0;
}

